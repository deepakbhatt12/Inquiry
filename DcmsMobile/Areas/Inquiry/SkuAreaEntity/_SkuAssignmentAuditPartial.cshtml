@model IList<DcmsMobile.Inquiry.Areas.Inquiry.SkuAreaEntity.LocationAuditModel>

<div class="table-responsive">
    <table class="table table-striped table-condensed table-bordered">
        <caption>
            <h3>Location Assignment Audit</h3>
        </caption>
        <thead>
            <tr>
                <th style="text-align:center">#</th>
                <th style="text-align:center">Action Performed</th>
                <th style="text-align:center">Created By</th>
                <th style="text-align:center">Date<span class="glyphicon glyphicon-sort-by-order-alt"></span></th>
                <th style="text-align:center">Module</th>
                <th style="text-align:center">UPC</th>
            </tr>
        </thead>
        <tbody>
            @for (int n = 0; n < Model.Count; n++)
            {
                <tr>
                    <td>@(n + 1)</td>
                    <td>@Html.DisplayFor(m => m[n].ActionPerformed)</td>
                    <td>@Html.DisplayFor(m => m[n].CreatedBy)</td>
                    <td>@Html.DisplayFor(m => m[n].DateCreated)</td>
                    <td>@Html.DisplayFor(m => m[n].ModuleCode)</td>
                    <td>@Html.DisplayFor(m => m[n].UpcCode)</td>
                </tr>
            }
        </tbody>
    </table>

</div>

